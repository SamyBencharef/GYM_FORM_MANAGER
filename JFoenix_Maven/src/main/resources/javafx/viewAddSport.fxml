<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="440.0" prefWidth="410.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymformmanager.controller.controllerAddSport">

    <children>
        <VBox layoutX="87.0" layoutY="14.0" prefHeight="386.0" prefWidth="410.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <JFXTextField fx:id="sportIntitule" focusColor="#ff5131" labelFloat="true" opacity="0.83" promptText="Nom du sport">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="sportEntraineur" focusColor="#ff5131" labelFloat="true" layoutX="20.0" layoutY="30.0" opacity="0.83" promptText="Entraîneur">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="sportPrixAnnee" focusColor="#ff5131" labelFloat="true" layoutX="10.0" layoutY="56.0" opacity="0.83" prefHeight="26.0" prefWidth="182.0" promptText="Prix à l'année">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="sportPrixMois" focusColor="#ff5131" labelFloat="true" layoutX="10.0" layoutY="82.0" opacity="0.83" promptText="Prix au mois">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </JFXTextField>
                <JFXTextField fx:id="sportPrixTrimestre" focusColor="#ff5131" labelFloat="true" layoutX="20.0" layoutY="168.0" opacity="0.83" promptText="Prix au trimestre">
                    <VBox.margin>
                        <Insets bottom="50.0" left="10.0" right="10.0" top="10.0" />
                    </VBox.margin>
                </JFXTextField>
                <HBox prefHeight="64.0" prefWidth="410.0">
                    <children>
                        <JFXButton onAction="#ajouterSport" prefHeight="64.0" prefWidth="500.0" styleClass="redbutton" stylesheets="@../style/style.css" text="Sauvegarder" />
                        <JFXButton layoutX="10.0" layoutY="10.0" onAction="#annulerAjoutSport" prefHeight="64.0" prefWidth="500.0" styleClass="redbutton" stylesheets="@../style/style.css" text="Annuler" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>


</AnchorPane>
